{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions":  {
    "ecmaVersion":  2018,
    "sourceType":  "module"
  },
  "env": {
    "browser": true,
     "node": true,
      "amd": true
  },
  "root": true,
  "rules": {
    "quotes": ["error", "double"] //クォーテーションがシングルのときエラー
  }
}
